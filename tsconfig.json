{
    "compilerOptions": {
      "outDir": "./dist/",
      "module": "ESNext",
      "target": "es5",

      "noImplicitAny": true,
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
    }

}